{
  "max_epochs": 500,
  "batch_size": 80,
  "keep_prob": 0.9,
  "max_length": 100,
  "num_buckets": 100,
  "lstm_num_layers": 8,
  "lstm_hidden_dim": 300,
  "dblstm": true,
  "orthonormal_init": true,
  "recurrent_dropout": true,
  "features": [
    {
      "name": "words",
      "base": true,
      "rank": 3,
      "dim": 100,
      "initializer": {
        "pkl_path": "vectors.pkl",
        "initializer_path": "data/vectors/glove.6B.100d.txt"
      },
      "function": {
        "name": "ConvNet",
        "window_size": 3,
        "num_filters": 100,
        "max_length": 15
      },
      "extractor": {
        "name": "lower",
        "key": "word"
      }
    },
    {
      "name": "markers",
      "rank": 2,
      "dim": 100,
      "extractor": {
        "name": "key",
        "key": "markers"
      }
    },
    {
      "name": "phrase_chars",
      "dim": 25,
      "keep_prob": 0.5,
      "left_padding": 2,
      "right_padding": 2,
      "rank": 3,
      "function": {
        "name": "ConvNet",
        "window_size": 3,
        "num_filters": 20,
        "max_length": 30
      },
      "extractor": {
        "name": "chars",
        "key": "word"
      }
    }
  ]
}